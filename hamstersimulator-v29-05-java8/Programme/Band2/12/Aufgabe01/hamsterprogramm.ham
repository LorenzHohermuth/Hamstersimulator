/*object-oriented program*/import java.lang.Math;

class f1 implements Funktion {
    public int f(int x) {
        return -1 * (int)Math.pow(x, 2) + 4 * x + 2;
    }
}



void main() {
    FunktionenHamster paul = new FunktionenHamster(Hamster
            .getStandardHamster());
    Funktion funk = new f1();
    paul.loeschen();
    paul.zeichneGraph(funk, -2 , -3);
    
}
