/*object-oriented program*/
Hamster paul = Hamster.getStandardHamster();
Hamster willi = new Hamster();
void main() {
    willi.init(0, 0, Hamster.SUED, 0);
    while(willi.getReihe() != paul.getReihe()) {
        willi.vor();
    }
    willi.linksUm();
    while(willi.getSpalte() != paul.getSpalte()) {
        willi.vor();
    }
}

